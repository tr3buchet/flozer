{
    "cookie_map": "lambda c: (not c and 'vif*.*') or 'vif%d.%d' % (c & 0xFFFF, c >> 16)",
    "match_map": {"0x15/0xff": "EGRESS(0x15)",
                  "0x1a/0xff": "INGRESS(0x1a)",
                  "0xb/0xff": "EGRESS(0xb)",
                  "0x10/0xff": "INGRESS(0x10)",
                  "0x1f/0xff": "EGRESS(0x1f)",
                  "0x24/0xff": "INGRESS(0x24)",
                  "0/0xff": "DEFAULT(0)"},
    "table_map": {"0": "BASE(0)",
                  "10": "INTER_VM(10)",
                  "20": "SECURITY_GROUP(20)",
                  "21": "STATIC_SECURITY_GROUP_1(21)",
                  "22": "STATIC_SECURITY_GROUP_2(22)",
                  "29": "REFLEXIVE_RULES(29)",
                  "30": "PORT_SECURITY(30)"},
    "action_map": {"resubmit(,": "resubmit",
                   "NXM_NX_REG0[0..7]": "reg0",
                   "load:0x15->": "load:EGRESS(0x15)->",
                   "load:0x1a->": "load:INGRESS(0x1a)->",
                   "load:0xb->": "load:EGRESS(0xb)->",
                   "load:0x10->": "load:INGRESS(0x10)->",
                   "load:0x1f->": "load:EGRESS(0x1f)->",
                   "load:0x24->": "load:INGRESS(0x24)->",
                   "load:0->": "load:DEFAULT(0)->"},
    "protocol": "OpenFlow11",
    "disable_unicode": "true",
    "sort": ["label", "n_packets"]
}
